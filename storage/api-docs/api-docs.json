        "/api/products": {
            "get": {
                "tags": [
                    "Product"
                ],
                "summary": "取得商品列表",
                "description": "取得商品列表 Description",
                "operationId": "product__get_products",
                "responses": {
                    "200": {
                        "description": "請求成功",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "success": {
                                            "description": "執行結果",
                                            "type": "boolean",
                                            "example": "true"
                                        },
                                        "message": {
                                            "description": "訊息",
                                            "type": "string",
                                            "example": "Products Retrieved Successfully."
                                        },
                                        "data": {
                                            "description": "商品列表資料",
                                            "type": "array",
                                            "items": {
                                                "properties": {
                                                    "id": {
                                                        "type": "number",
                                                        "example": "1"
                                                    },
                                                    "name": {
                                                        "type": "string",
